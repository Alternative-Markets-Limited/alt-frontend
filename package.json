{
  "name": "alt-frontend",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://altappng.herokuapp.com/",
  "engines": {
    "node": "13.2.0",
    "npm": "6.13.1"
  },
  "dependencies": {
    "@ant-design/icons": "^4.0.3",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
